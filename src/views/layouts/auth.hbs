    <script>
        document.addEventListener("DOMContentLoaded", function() {
            const currentPage = window.location.pathname.split("/").pop();

            let customCss = document.createElement("link");
            customCss.rel = "stylesheet";

            switch (currentPage) {
                case "login":
                    customCss.href = "/css/auth/login.css";
                    break;
                case "register":
                    customCss.href = "/css/auth/register.css";
                    break;
                  case "forgot-password":
                      customCss.href = "/css/auth/forgot-password.css";
                      break;
                case "activate":
                    customCss.href = "/css/auth/activate-account.css";
                    break;
                default:
                    break;
            }

            document.head.appendChild(customCss);
        });
    </script>
